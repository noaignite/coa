{
  "name": "create-ignite-app",
  "version": "2.0.0",
  "private": false,
  "author": "NoA Ignite",
  "description": "NoA Ignite App Scaffolding",
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/create-ignite-app.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noaignite/create-ignite-app/issues"
  },
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "storybook": "start-storybook -s ./public -p 3001 --ci",
    "build-storybook": "build-storybook -s ./public -o .storybook-static",
    "lint": "eslint . --cache --report-unused-disable-directives",
    "lint:ci": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ],
    "*.{json,md,css,html}": [
      "yarn prettier:fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.6.0",
    "@noaignite/oui": "^3.3.0",
    "@react-hook/size": "^2.1.2",
    "clsx": "^1.1.0",
    "embla-carousel-react": "^5.0.1",
    "es2015-i18n-tag": "^1.6.1",
    "next": "^12.1.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-constant-elements": "^7.10.1",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/cz-commitlint": "^16.2.3",
    "@emotion/babel-plugin": "^11.7.2",
    "@next/bundle-analyzer": "^12.1.4",
    "@noaignite/eslint-config": "^0.2.2",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/react": "^6.4.20",
    "babel-plugin-i18n-tag-translate": "^2.0.0",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.12.0",
    "eslint-config-next": "^12.1.4",
    "eslint-import-resolver-webpack": "^0.13.2",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "webpack-merge": "^5.2.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
