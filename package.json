{
  "name": "create-ignite-app",
  "version": "1.6.0",
  "private": false,
  "author": "NoA Ignite",
  "description": "NoA Ignite App Scaffolding",
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/create-ignite-app.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noaignite/create-ignite-app/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "dev": "next",
    "release": "standard-version",
    "lint": "eslint . --cache --report-unused-disable-directives",
    "lint:ci": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "provision:production": "op inject -i .env.provision.production -o .env.local",
    "provision:stage": "op inject -i .env.provision.stage -o .env.local",
    "storybook": "storybook dev -p 3001 --ci",
    "storybook:build": "storybook build -o .storybook-static",
    "storybook:test": "test-storybook --url http://127.0.0.1:3001",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ],
    "*.{json,md,css,html}": [
      "yarn prettier:fix"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/lab": "^5.0.0-alpha.118",
    "@mui/material": "^5.11.7",
    "@noaignite/oui": "^4.0.0",
    "clsx": "^1.2.1",
    "es2015-i18n-tag": "^1.6.1",
    "next": "^13.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-constant-elements": "^7.10.1",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@emotion/babel-plugin": "^11.9.2",
    "@next/bundle-analyzer": "^13.1.6",
    "@noaignite/eslint-config-typescript": "^0.4.0",
    "@storybook/addon-a11y": "^7.0.3",
    "@storybook/addon-essentials": "^7.0.3",
    "@storybook/nextjs": "^7.0.3",
    "@storybook/react": "^7.0.3",
    "@storybook/test-runner": "^0.10.0",
    "babel-plugin-i18n-tag-translate": "^2.0.0",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.12.0",
    "eslint-config-next": "^13.1.6",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "standard-version": "^9.3.2",
    "storybook": "^7.0.3",
    "webpack-merge": "^5.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
